{
  "name": "thepcreview",
  "version": "1.0.0",
  "description": "The PC Review",
  "main": "index.js",
  "homepage": ".",
  "scripts": {
    "webpack:dev": "npx webpack --config webpack.dev.js --watch",
    "webpack:prod": "npx webpack -p --mode=production --config webpack.prod.js",
    "start": "webpack-dev-server --mode development --inline --progress --watch --watch-poll --hot",
    "devasprod": "webpack-dev-server --open -p --mode=production --config webpack.prod.js",
    "develop": "webpack-dev-server --open --mode development --config webpack.config.js",
    "deploy": "npx webpack -p --mode=production --config webpack.prod.js && gh-pages -d app"
  },
